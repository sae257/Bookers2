<div class="container"> 
 <div class="row">
  <div class="col-md-3">
    <table class="table table-striped">
      
<%= render 'books/list', user: @user, book: @book %>
</table>
  </div>

<div class="col-md-8 offset-md-1">
   <h2>Users</h2>
   <table class="table table-striped">
     <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
     </tr>
<% @users.each do |f| %>
      <tr>
        <td><%= image_tag f.get_profile_image(100,100) %></td>
        <td><%= f.name %></td>
        <td><%= link_to "Show", user_path(f.id) %></td>
      </tr>
 <% end %>
</table>
</div>
</div>
</div>
